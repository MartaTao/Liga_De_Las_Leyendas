
<%- include("templates/cabecera.ejs") %>
<title>Nueva skin</title>
</head>
<body>
<div class="container">
    <div><a class="btn btn-success btn-block my-4" href="/">Volver</a></div>
    <h1>Añadir nuevo Modo</h1>
    <form action="/skins" class="my-2" method="POST">
        <input type="text" placeholder="Nombre de la skin" class="form-control my-2" name="Nombre">
        <input type="text" placeholder="Url de la imágen" class="form-control my-2" name="Img">
        <input type="text" placeholder="Universo" class="form-control my-2" name="Universo">
        <input type="text" placeholder="Precio" class="form-control my-2" name="Precio">
        <input type="text" placeholder="Tier" class="form-control my-2" name="Tier">
        <button type="submit" class="btn btn-dark btn-block">
            Agregar
        </button>
    </form>
</div>
